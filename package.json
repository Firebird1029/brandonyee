{
	"name": "brandonyee.io",
	"version": "1.0.0",
	"description": "https://brandonyee.io",
	"scripts": {
		"build": "pug --out ./ src/",
		"dev": "npm-run-all -p dev-pug dev-bs",
		"dev-pug": "pug --watch src/ --out ./ src/ --obj src/data.json -s",
		"dev-pyserver": "python -m http.server",
		"dev-bs": "browser-sync start --config bs-config.js",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Firebird1029/brandonyee.git"
	},
	"author": "Brandon Yee",
	"license": "ISC",
	"homepage": "https://github.com/Firebird1029/brandonyee",
	"devDependencies": {
		"browser-sync": "^2.26.13",
		"npm-run-all": "^4.1.5",
		"pug-cli": "^1.0.0-alpha6"
	}
}
